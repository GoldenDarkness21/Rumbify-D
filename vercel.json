{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/codes/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/parties/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/app1/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/app2/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
